@{
    ViewBag.Title = "Adult Programme booking Summer 2015";
    Layout = "~/Views/_LayoutPage.cshtml";
}

<link href="@Url.Content("~/CSS/BookingButtons.css")" rel="stylesheet" />

<div class="container content">


    <div class="headline"><h2>How to book a course place - Summer 2015</h2></div>

    <div class="row">

        <p>
            Please click below on the course that you wish to book for.
            Payment can be made using paypal or by using a debit or credit card.
        </p>
        <p>
            If you are booking before the earlybird discount deadline, your earlybird discount will be applied
            automatically when you click "add to cart"
        </p>
        <p>
            The cost of all 12 1-hour lessons is £120 for the term or £108 if you book before the 7th March.
            <br />
            If your are not a member of Wigmore Tennis Club, you will be charged an additional £30.00 guest fee.
        </p>
    </div>
    <div>
        <form target="paypal" action="https://www.paypal.com/cgi-bin/webscr" method="post">
            <input type="hidden" name="cmd" value="_s-xclick">
            <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----">
            <input type="image" src="https://www.paypalobjects.com/en_GB/i/btn/btn_viewcart_LG.gif" border="0" name="submit" alt="PayPal – The safer, easier way to pay online.">
            <img alt="" border="0" src="https://www.paypalobjects.com/en_GB/i/scr/pixel.gif" width="1" height="1">
        </form>

    </div>
    <div class="container">
        @Html.Partial("~/Views/BookingForms/_Summer2015Adults.cshtml")
    </div>
</div>


