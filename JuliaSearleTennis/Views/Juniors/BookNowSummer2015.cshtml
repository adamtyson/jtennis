@{
    ViewBag.Title = "Junior Programme booking Summer 2015";
    Layout = "~/Views/_LayoutPage.cshtml";
}

<link href="@Url.Content("~/CSS/BookingButtons.css")" rel="stylesheet" />
<link href="@Url.Content("~/CSS/JuniorBookingButtons.css")" rel="stylesheet" />


<div class="container content">


    <div class="headline"><h2>How to book a course place - Summer 2015</h2></div>

    <div class="row">

        <p>
            Please click below on the course that you wish to book for.
            Payment can be made using paypal or by using a debit or credit card.
        </p>
        <p>
            If you are booking before the earlybird discount deadline (applies to the group programme only), your earlybird discount will be applied
            automatically when you click "add to cart"
        </p>
        <p>
            The cost of all 12 1-hour group lessons is £120 for the term or £108 if you book before the 7th March. Monday courses have 1 fewer session due to the bank holiday and are priced at £110 (£99 if you book before the 7th March).

            <br />
            If your child is not a member of Wigmore Tennis Club, you will be charged an additional £18.00 guest fee.
            Alternatively, they can become a club member by filling in the
            <a href="http://wigmoretennis.clubsolution.co.uk/proc_bruger.asp"> application form  here.</a>
            Junior membership is £20 for children age 9 & under or £60 for children age 10-17yrs.
        </p>
    </div>
    <div>
        <form target="paypal" action="https://www.paypal.com/cgi-bin/webscr" method="post">
            <input type="hidden" name="cmd" value="_s-xclick">
            <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----">
            <input type="image" src="https://www.paypalobjects.com/en_GB/i/btn/btn_viewcart_LG.gif" border="0" name="submit" alt="PayPal – The safer, easier way to pay online.">
            <img alt="" border="0" src="https://www.paypalobjects.com/en_GB/i/scr/pixel.gif" width="1" height="1">
        </form>

    </div>
    <div class="row">
        <div class="container">
            <div class="tab-v1">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#tots" data-toggle="tab">Tots</a></li>
                    <li><a href="#mini" data-toggle="tab">Mini</a></li>
                    <li><a href="#junior" data-toggle="tab">Junior</a></li>

                </ul>

                <div class="tab-content">
                    <div class="tab-pane fade in active" id="tots">
                        <div class="row">
                            <div class="col-md-9">
                                <div>
                                    @Html.Partial("~/Views/BookingForms/_Summer2015tots.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane fade in" id="mini">
                        <div class="row">
                            <div class="col-md-9">
                                <div>
                                    @Html.Partial("~/Views/BookingForms/_Summer2015mini.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="tab-pane fade in" id="junior">
                        <div class="row">
                            <div class="col-md-9">
                                <div>
                                    @Html.Partial("~/Views/BookingForms/_Summer2015Juniors.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
  

